{
  "kind": "collectionType",
  "collectionName": "vehicle_types",
  "info": {
    "singularName": "vehicle-type",
    "pluralName": "vehicle-types",
    "displayName": "Vehicle Type",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "digit": {
      "type": "enumeration",
      "enum": [
        "first",
        "last"
      ]
    },
    "schedules": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::peak-hour.peak-hour",
      "inversedBy": "vehicle_types"
    }
  }
}
